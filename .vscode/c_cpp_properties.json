{
    "configurations": [
        {
            "name": "Cross Compile Raspberry Pi 5",
            "includePath": [
                "${workspaceFolder}/include",
                "${workspaceFolder}/third_party/paho.mqtt.cpp/src",
                "${workspaceFolder}/third_party/paho.mqtt.c/src",
                "${env:HOME}/sysroot-aarch64/usr/include",
                "${env:HOME}/sysroot-aarch64/usr/include/aarch64-linux-gnu",
                "${env:HOME}/sysroot-aarch64/usr/include/c++/12",
                "${env:HOME}/sysroot-aarch64/usr/include/aarch64-linux-gnu/c++/12",
                "${env:HOME}/sysroot-aarch64/usr/include/c++/12/bits",
                "${env:HOME}/sysroot-aarch64/usr/include/aarch64-linux-gnu/c++/12/bits"
            ],
            "defines": [],
            "compilerPath": "/usr/bin/aarch64-linux-gnu-g++",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "linux-gcc-arm64",
            "compilerArgs": [
                "--sysroot=${env:HOME}/sysroot-aarch64"
            ]
        }
    ],
    "version": 4
}
